{
  "name": "QuickShift For Chrome",
  "version": "1.2.1",
  "description": "Hotkeys! Move Chrome tabs to a different window or to a new location within the current window using your keyboard.",
  "permissions": ["tabs"],
  "background_page": "background.html",
  "options_page": "options.html",
  "icons":
  {
	"16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png"
  },
  "content_scripts":
  [
	{
		"matches": ["<all_urls>"],
		"js": ["script.js"],
		"run_at": "document_start",
		"all_frames": true
	}
  ]
}